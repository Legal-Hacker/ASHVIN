/*Write a C program to swipe two Number */
#include<stdio.h>
int main()
{
    int a,b,temp;
    printf("\nEnter A=");
    scanf("%d",&a);
    printf("\nEnter B=");
    scanf("%d",&b);
    temp=a;
    a=b;
    b=temp;
    /*
    Print After Swiping*/
    printf("\nA=%d",a);
    printf("\nB=%d",b);
    return 0;
}
