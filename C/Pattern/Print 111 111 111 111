#include<stdio.h>
int main()
{
    int i,j;
    for(i=1;i<=4;i++)/*Outer Loop*/
    {
        for(j=1;j<=3;j++)/*Inner loop*/
        {
            printf("%d",1);
        }
    
        printf("\n");
    }
    
}


Solucton 2

#include<stdio.h>
int main()
{
    int i,j;
    for(i=4;i>=1;i--)/*Outer Loop*/
    {
        for(j=3;j>=1;j--)/*Inner loop*/
        {
            printf("%d",1);
        }
    
        printf("\n");
    }
    
}
