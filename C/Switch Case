#include<stdio.h>

void main()
{   
//Switch case

    int e=30;
    switch(e)
    {
    case 10: printf("value is 10");
    break;
    case 20: printf("value is 20");
    break;
    case 30: printf("value is 30");
    break;
    default: printf("value is not persent");
    break;
    }
    getchar();
}
