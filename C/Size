#include<stdio.h>
#include<stdbool.h>

void main()
{   
    int a;
    float b;
    char c;
    double d;
    printf("size of int is %ld",sizeof(a));
    printf("\nsize of float is %ld",sizeof(b));
    printf("\nsize of char is %ld",sizeof(c));
    printf("\nsize of double is %ld",sizeof(d));
    getchar();
}
