
#include<stdio.h>
int main()
{
    /*Using if*/
    int m;
    printf("\nEnter Marks");
    scanf("\n%d",&m);
    if(m<0||m>100)
    printf("\nInvalid");
    else if(m>=75)
    printf("\nDistinction");
    else if(m>=60)
    printf("\nFirst");
    else if(m>=45)
    printf("\nSecond");
    else if(m>=30)
    printf("\nThird");
    else
    printf("\nFail");
    return 0;
}
